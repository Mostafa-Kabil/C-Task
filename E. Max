#include <iostream>
using namespace std;
int main() {
    cout<<"Enter how many numbers do you want to enter"<<endl;
    int n;
    cin >> n;
    cout<<"Enter the first number"<<endl;
    int num;
    cin >> num;     
    int maximum = num;  
    for (int i = 1; i < n; i++) {
        cin >> num;
        if (num > maximum) {
            maximum = num;
        }
    }

    cout << maximum << endl;
    return 0;
}
